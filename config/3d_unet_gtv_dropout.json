{
    "train_params": {
        "epochs": 200,
        "callbacks": [
            {
                "class_name": "EarlyStopping",
                "config": {
                    "monitor": "val_loss",
                    "patience": 30
                }
            }
        ]
    },
    "dataset_params": {
        "class_name": "H5Reader",
        "config": {
            "filename": "../datasets/ous_hnc_3d.h5",
            "batch_size": 2,
            "batch_cache": 1,
            "x_name": "input",
            "y_name": "target",
            "train_folds": [
                0
            ],
            "val_folds": [
                1
            ],
            "test_folds": [
                1
            ],
            "shuffle": true,
            "preprocessors": [
                {
                    "class_name": "CombineMaskPreprocessor",
                    "config": {
                        "mask_channels": [
                            0,
                            1
                        ]
                    }
                },
                {
                    "class_name": "HounsfieldWindowingPreprocessor",
                    "config": {
                        "window_center": 70,
                        "window_width": 200,
                        "channel": 0
                    }
                },
                {
                    "class_name": "ImageNormalizerPreprocessor",
                    "config": {
                        "vmin": [
                            -100,
                            0
                        ],
                        "vmax": [
                            100,
                            10
                        ]
                    }
                }
            ],
            "augmentations": {
                "class_name": "ImageAugmentation3D",
                "config": {
                    "rotation_range": 90,
                    "zoom_range": [
                        0.8,
                        1.2
                    ],
                    "shift_range": [
                        10,
                        10,
                        10
                    ],
                    "flip_axis": 1
                }
            }
        }
    },
    "input_params": {
        "shape": [
            176,
            192,
            256,
            2
        ]
    },
    "model_params": {
        "loss": {
            "class_name": "BinaryFbetaLoss"
        },
        "optimizer": {
            "class_name": "adam",
            "config": {
                "learning_rate": 0.0001
            }
        },
        "metrics": [
            {
                "class_name": "BinaryFbeta"
            },
            {
                "class_name": "Dice"
            }
        ]
    },
    "architecture": {
        "type": "Vnet",
        "layers": [
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 32,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 32,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                },
                "name": "conv1"
            },
            {
                "class_name": "Dropout",
                "config": {
                    "rate": 0.02
                }
            },
            {
                "class_name": "MaxPooling3D"
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 64,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 64,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                },
                "name": "conv2"
            },
            {
                "class_name": "Dropout",
                "config": {
                    "rate": 0.02
                }
            },
            {
                "class_name": "MaxPooling3D"
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 128,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 128,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                },
                "name": "conv3"
            },
            {
                "class_name": "Dropout",
                "config": {
                    "rate": 0.02
                }
            },
            {
                "class_name": "MaxPooling3D"
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 256,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 256,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                },
                "name": "conv4"
            },
            {
                "class_name": "Dropout",
                "config": {
                    "rate": 0.02
                }
            },
            {
                "class_name": "MaxPooling3D"
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 512,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 512,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Dropout",
                "config": {
                    "rate": 0.02
                }
            },
            {
                "name": "upconv4",
                "class_name": "Conv3DTranspose",
                "config": {
                    "filters": 256,
                    "kernel_size": 3,
                    "strides": 2,
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 256,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                },
                "inputs": [
                    "conv4",
                    "upconv4"
                ]
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 256,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Dropout",
                "config": {
                    "rate": 0.02
                }
            },
            {
                "name": "upconv3",
                "class_name": "Conv3DTranspose",
                "config": {
                    "filters": 128,
                    "kernel_size": 3,
                    "strides": 2,
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 128,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                },
                "inputs": [
                    "conv3",
                    "upconv3"
                ]
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 128,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Dropout",
                "config": {
                    "rate": 0.02
                }
            },
            {
                "name": "upconv2",
                "class_name": "Conv3DTranspose",
                "config": {
                    "filters": 64,
                    "kernel_size": 3,
                    "strides": 2,
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 64,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                },
                "inputs": [
                    "conv2",
                    "upconv2"
                ]
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 64,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Dropout",
                "config": {
                    "rate": 0.02
                }
            },
            {
                "name": "upconv1",
                "class_name": "Conv3DTranspose",
                "config": {
                    "filters": 32,
                    "kernel_size": 3,
                    "strides": 2,
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 32,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                },
                "inputs": [
                    "conv1",
                    "upconv1"
                ]
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 32,
                    "kernel_size": 3,
                    "activation": "swish",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 1,
                    "kernel_size": 3,
                    "activation": "sigmoid",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                }
            }
        ]
    }
}
